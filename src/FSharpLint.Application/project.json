{
  "name": "FSharpLint.Application",
  "version": "1.0.0-*",
  "buildOptions": {
    "compilerName": "fsc",
    "compile": {
      "includeFiles": [
        "LintWarning.fsi",
        "LintWarning.fs",
        "Lint.fsi",
        "Lint.fs",
        "FSharpLintWorker.fs"
      ]
    }
  },
  "dependencies": {
    "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-160509",
    "FSharp.Compiler.Service.netcore": "1.0.0-alpha-00001",
    "FSharp.Compiler.Service.ProjectCracker.netcore": "1.0.0-alpha-00001",
    "FParsec": "1.0.2",
    "NETStandard.Library": "1.5.0-rc2-*",
    "FSharpLint.Framework":{
      "version":"1.0.0",
      "target":"project"
    },
    "FSharpLint.Rules":{
      "version":"1.0.0",
      "target":"project"
    }
  },
  "tools": {
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview1-*",
      "imports": [
        "dnxcore50",
        "portable-net45+win81",
        "netstandard1.3"
      ]
    }
  },
  "frameworks": {
    "netstandard1.5": {
      "imports": [
        "portable-net45+win8",
        "dnxcore50"
      ]
    }
  }
}